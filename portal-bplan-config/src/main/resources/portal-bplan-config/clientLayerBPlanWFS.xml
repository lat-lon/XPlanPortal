<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <bean class="org.geomajas.configuration.client.ClientVectorLayerInfo"
        id="clientLayerBPlanWFS">
    <property name="serverLayerId" value="layerBPlanWFS"/>
    <property name="label" value="Bebauungspläne"/>
    <property name="visible" value="true"/>
    <property name="maximumScale" value="1:1"/>
    <property name="minimumScale" value="1:500000000"/>
    <property name="namedStyleInfo" ref="BPlanWFSStyleInfo"/>
  </bean>
  <bean class="org.geomajas.configuration.client.ClientVectorLayerInfo"
        id="clientLayerBaugebTeilWFS">
    <property name="serverLayerId" value="layerBaugebTeilWFS"/>
    <property name="label" value="Baugebietsteilflächen"/>
    <property name="visible" value="true"/>
    <property name="maximumScale" value="1:1"/>
    <property name="minimumScale" value="1:500000000"/>
    <property name="namedStyleInfo" ref="BPlanWFSStyleInfo"/>
  </bean>

</beans>
	
